    <datafield tag="245" ind1="1" ind2="0">
        <subfield code="a">Pollen selection by feral honey bee (Hymenoptera: Apidae) colonies in a
            coastal prairie landscape</subfield>
    </datafield>
    <datafield tag="300" ind1=" " ind2=" ">
        <subfield code="a">p. 727-239.</subfield>
    </datafield>
    <datafield tag="500" ind1=" " ind2=" ">
        <subfield code="a">Includes references</subfield>
    </datafield>
    <datafield tag="500" ind1=" " ind2=" ">
